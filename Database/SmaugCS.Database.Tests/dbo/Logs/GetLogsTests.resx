  <data name="GetLogs_RowCount_Test_TestAction.SqlScript" xml:space="preserve">
    <value>-- database unit test for dbo.cp_GetLogs
DECLARE @RC AS INT, @logType AS TINYINT, @fromDate AS DATETIME, @toDate AS DATETIME;

SELECT @RC = 0,
       @logType = 1,
       @fromDate = DATEADD(DAY, -1, GetDate()),
       @toDate = DATEADD(DAY, 1, GetDate());

EXECUTE @RC = [dbo].[cp_GetLogs] @logType, @fromDate, @toDate;

SELECT @RC AS RC;

</value>
